        {%- comment -%}
          Live region for announcing updated price and availability to screen readers
        {%- endcomment -%}
        <p class="visually-hidden" data-product-status
          aria-live="polite"
          role="status"
        ></p>

        {%- comment -%}
          Live region for announcing that the product form has been submitted and the
          product is in the process being added to the cart
        {%- endcomment -%}
        <p class="visually-hidden" data-loader-status
          aria-live="assertive"
          role="alert"
          aria-hidden="true"
        >{{ 'products.product.loader_label' | t }}</p>

        <div class="product-single__icon-text">
          <span class="visually-hidden">Produktvorteile</span>
          <ul class="product-list-icons" style="list-style: none;">
            {% for i in (1..4) %}
            {% assign featuredProductIcon = 'icon_' | append: i %}
            {% assign featuredProductTitle = 'content_' | append: i %}
            <li aria-hidden="true">
              <figure class="product-benefit-icon">
                {% if section.settings[featuredProductIcon] != blank %}<img style="border-radius:50%;" class="lazyload" src="{{ section.settings[featuredProductIcon] | img_url: '80x' }}" alt="{{ section.settings[featuredProductIcon].alt }}" />{% endif %}
                {% if section.settings[featuredProductTitle] != blank %}<figcaption class="h6">{{ section.settings[featuredProductTitle] }}</figcaption>{% endif %}
              </figure>
            </li>
            {% endfor %}
          </ul>
        </div>

        <div class="product-single__icon-text">
          <span class="visually-hidden">Produktvorteile</span>
          <ul class="product-list-icons" style="list-style: none;">
            {% for i in (1..4) %}
            {% assign featuredProductIcon = 'icon_2_' | append: i %}
            {% assign featuredProductTitle = 'content_2_' | append: i %}
            <li aria-hidden="true">
              <figure class="product-benefit-icon">
                {% if section.settings[featuredProductIcon] != blank %}<img style="border-radius:50%;" class="lazyload" src="{{ section.settings[featuredProductIcon] | img_url: '80x' }}" alt="{{ section.settings[featuredProductIcon].alt }}" />{% endif %}
                {% if section.settings[featuredProductTitle] != blank %}<figcaption class="h6">{{ section.settings[featuredProductTitle] }}</figcaption>{% endif %}
              </figure>
            </li>
            {% endfor %}
          </ul>
        </div>

        {% if section.settings.show_shipping_time %}
        <p class="accent-background h3 a-list-item" style="text-align:center;">
          <span class="visually-hidden">Versandzeit</span>
          🚚 Versandfertig in 12 - 24 Stunden 📫
        </p>
        {% endif %}

        <div class="product-single__icon-text">
          <span class="visually-hidden">Versandvorteile</span>
          <ul class="product-shipping-icons" style="list-style: none;">
            {% for i in (1..4) %}
            {% assign featuredProductShippingIcon = 'shipping_icon_' | append: i %}
            {% assign featuredProductShippingTitle = 'shipping_content_' | append: i %}
            <li aria-hidden="true">
              <figure class="product-shipping-benefit-icon">
                {% if section.settings[featuredProductShippingIcon] != blank %}<img class="lazyload" src="{{ section.settings[featuredProductShippingIcon] | img_url: '50x' }}" alt="{{ section.settings[featuredProductShippingIcon].alt }}" />{% endif %}
                {% if section.settings[featuredProductShippingTitle] != blank %}<figcaption class="h4">{{ section.settings[featuredProductShippingTitle] }}</figcaption>{% endif %}
              </figure>
            </li>
            {% endfor %}
          </ul>
        </div>

        {% if section.settings.show_share_buttons %}
        {% include 'social-sharing', share_title: product.title, share_permalink: product.url, share_image: product.featured_media %}
        {% endif %}

        <div class="product-single__description rte">
          {{ product.description }}
        </div>
    </div>
</div>

{% comment %}Start automatically added Judge.me widget{% endcomment %}
{% render 'judgeme_widgets', widget_type: 'judgeme_review_widget', concierge_install: true, product: product %}
{% comment %}End automatically added Judge.me widget{% endcomment %}
</div>

<div class="page-width">
    {% include 'collection-menu' %}
</div>

{% if section.settings.blendout_etrustwidget %}
<style>
  etrusted-widget {
  display: none!important;
  visibility: hidden!important;
}
</style>
{% endif %}

{% unless product == empty %}
  <script type="application/json" id="ProductJson-{{ section.id }}">
    {{ product | json }}
  </script>
  <script type="application/json" id="ModelJson-{{ section.id }}">
    {{ product.media | where: 'media_type', 'model' | json }}
  </script>
{% endunless %}

{% stylesheet %}
.main-content {
  padding-top: 25px;
}
.product-form {
    width: auto;
    padding-top: 0;
}
.product-form__controls-group-btn {
  width: 100%;
}
.product-form__controls-group-btn .product-form__item--no-variants {
  max-width: inherit;
}
.product-form__group {
  display: flex;
  flex-wrap: nowrap;
  flex-direction: row;
  align-content: center;
  justify-content: flex-start;
}
.btn--submit-new {
  line-height: 2;
  height: 45px;
  margin: 26px auto 0px;
  padding: 0px 20px;
  width: 100%;
}
@media only screen and (min-width: 990px) {
  .product-form__item {
      flex-basis: 100%;
      padding: 0 5px;
  }
}
ul.product-list-icons {
  margin: 1rem auto 0;
  display: flex;
  flex-direction: row;
  align-items: baseline;
  justify-content: center;
}
ul.product-list-icons > li {
  min-width: 25%;
}
ul.product-list-icons h4 {
  margin-bottom: 0;
}
ul.product-shipping-icons {
  margin: 1rem;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
}
ul.product-shipping-icons > li {
  flex-basis: 50%;
  margin: 5px auto;
}
ul.product-shipping-icons > li > figure > figcaption {
  margin-bottom: 0;
  line-height: 48px;
  width: 100%;
  text-align: center;
  font-size: 15px;
  font-weight: 500;
}
figure.product-benefit-icon {
  text-align: center;
  margin-block-start: 0;
  margin-block-end: 0;
  margin-inline-start: 0;
  margin-inline-end: 0;
  width: 100%;
  height: auto;
}
figure.product-shipping-benefit-icon {
  display: flex;
  align-items: flex-end;
  margin-block-start: 0;
  margin-block-end: 0;
  margin-inline-start: 0;
  margin-inline-end: 0;
  width: 100%;
  height: auto;
}
figure.product-shipping-benefit-icon > .h4 {
  font-weight: bold;
}
.flex-row {
  margin: 3px 4px 1rem 4px;
  display: flex;
  justify-content: space-between;
}
.accent-background {
  border: 1px solid #007a7b5c;
  background: #f5f5f5;
  padding: 15px 0;
  border-radius: 7px;
  font-size: 20px;
  font-weight: 400;
}
.social-sharing {
  justify-content: center;
}
@media only screen and (max-width: 471px) {
  .announcement-bar__message {
    font-size: 12px;
  }
  .product-single__title {
    font-size: 1.9em;
  }
  ul.product-list-icons {
    flex-wrap: wrap;
  }
  ul.product-list-icons > li {
    flex: 50%;
  }
  .btn--submit-new {
    margin: 24px auto 0px 8px;
    padding: 0 14px;
  }
  ul.product-shipping-icons > li > figure.product-shipping-benefit-icon {
    align-items: center;
  }
  ul.product-shipping-icons > li > figure.product-shipping-benefit-icon > figcaption {
    line-height: 24px;
  }
  ul.product-shipping-icons {
    margin: 1rem auto;
  }
  ul.product-shipping-icons > li {
    flex-basis: 46%;
    margin: 7px auto;
  }
  .flex-row {
    margin: 4px 0px;
    flex-wrap: wrap;
  }
  .flex-row li {
    max-width: 29px;
  }
  .accent-background {
    font-size: 14px;
    font-weight: 400;
  }
  figure.product-shipping-benefit-icon {
    background: #f5f5f5;
    padding: 0 7px;
    border-radius: 7px;
  }
}
div.product-single__description > div > table > tbody > tr > td {
  vertical-align: top;
  border: 1px solid transparent!important;
}
