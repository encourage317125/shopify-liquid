        <div class="product-single__icon-text">
          <span class="visually-hidden">Produktvorteile</span>
          <ul class="product-list-icons" style="list-style: none;">
            {% for i in (1..4) %}
            {% assign featuredProductIcon = 'icon_' | append: i %}
            {% assign featuredProductTitle = 'content_' | append: i %}
            <li aria-hidden="true">
              <figure class="product-benefit-icon">
                {% if section.settings[featuredProductIcon] != blank %}<img style="border-radius:50%;" class="lazyload" src="{{ section.settings[featuredProductIcon] | img_url: '80x' }}" alt="{{ section.settings[featuredProductIcon].alt }}" />{% endif %}
                {% if section.settings[featuredProductTitle] != blank %}<figcaption class="h6">{{ section.settings[featuredProductTitle] }}</figcaption>{% endif %}
              </figure>
            </li>
            {% endfor %}
          </ul>
        </div>

        <div class="product-single__icon-text">
          <span class="visually-hidden">Produktvorteile</span>
          <ul class="product-list-icons" style="list-style: none;">
            {% for i in (1..4) %}
            {% assign featuredProductIcon = 'icon_2_' | append: i %}
            {% assign featuredProductTitle = 'content_2_' | append: i %}
            <li aria-hidden="true">
              <figure class="product-benefit-icon">
                {% if section.settings[featuredProductIcon] != blank %}<img style="border-radius:50%;" class="lazyload" src="{{ section.settings[featuredProductIcon] | img_url: '80x' }}" alt="{{ section.settings[featuredProductIcon].alt }}" />{% endif %}
                {% if section.settings[featuredProductTitle] != blank %}<figcaption class="h6">{{ section.settings[featuredProductTitle] }}</figcaption>{% endif %}
              </figure>
            </li>
            {% endfor %}
          </ul>
        </div>

        {% if section.settings.show_shipping_time %}
        <p class="accent-background h3 a-list-item" style="text-align:center;">
          <span class="visually-hidden">Versandzeit</span>
