    {{ product.media | where: 'media_type', 'model' | json }}
  </script>
{% endunless %}

{% stylesheet %}
.main-content {
  padding-top: 25px;
}
.product-form {
    width: auto;
    padding-top: 0;
}
.product-form__controls-group-btn {
  width: 100%;
}
.product-form__controls-group-btn .product-form__item--no-variants {
  max-width: inherit;
}
.product-form__group {
  display: flex;
  flex-wrap: nowrap;
  flex-direction: row;
  align-content: center;
  justify-content: flex-start;
}
.btn--submit-new {
  line-height: 2;
  height: 45px;
  margin: 26px auto 0px;
  padding: 0px 20px;
  width: 100%;
}
@media only screen and (min-width: 990px) {
  .product-form__item {
      flex-basis: 100%;
      padding: 0 5px;
  }
}
ul.product-list-icons {
  margin: 1rem auto 0;
  display: flex;
  flex-direction: row;
  align-items: baseline;
  justify-content: center;
}
ul.product-list-icons > li {
  min-width: 25%;
}
ul.product-list-icons h4 {
  margin-bottom: 0;
}
ul.product-shipping-icons {
  margin: 1rem;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
}
ul.product-shipping-icons > li {
  flex-basis: 50%;
  margin: 5px auto;
}
ul.product-shipping-icons > li > figure > figcaption {
  margin-bottom: 0;
  line-height: 48px;
  width: 100%;
  text-align: center;
  font-size: 15px;
  font-weight: 500;
}
figure.product-benefit-icon {
  text-align: center;
  margin-block-start: 0;
  margin-block-end: 0;
  margin-inline-start: 0;
  margin-inline-end: 0;
  width: 100%;
  height: auto;
}
figure.product-shipping-benefit-icon {
  display: flex;
  align-items: flex-end;
  margin-block-start: 0;
  margin-block-end: 0;
  margin-inline-start: 0;
  margin-inline-end: 0;
  width: 100%;
  height: auto;
}
figure.product-shipping-benefit-icon > .h4 {
  font-weight: bold;
}
.flex-row {
  margin: 3px 4px 1rem 4px;
  display: flex;
  justify-content: space-between;
}
.accent-background {
  border: 1px solid #007a7b5c;
  background: #f5f5f5;
  padding: 15px 0;
  border-radius: 7px;
  font-size: 20px;
  font-weight: 400;
}
.social-sharing {
  justify-content: center;
}
@media only screen and (max-width: 471px) {
  .announcement-bar__message {
    font-size: 12px;
  }
  .product-single__title {
    font-size: 1.9em;
  }
  ul.product-list-icons {
