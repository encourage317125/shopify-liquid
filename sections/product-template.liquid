          <span class="visually-hidden">Versandvorteile</span>
          <ul class="product-shipping-icons" style="list-style: none;">
            {% for i in (1..4) %}
            {% assign featuredProductShippingIcon = 'shipping_icon_' | append: i %}
            {% assign featuredProductShippingTitle = 'shipping_content_' | append: i %}
            <li aria-hidden="true">
              <figure class="product-shipping-benefit-icon">
                {% if section.settings[featuredProductShippingIcon] != blank %}<img class="lazyload" src="{{ section.settings[featuredProductShippingIcon] | img_url: '50x' }}" alt="{{ section.settings[featuredProductShippingIcon].alt }}" />{% endif %}
